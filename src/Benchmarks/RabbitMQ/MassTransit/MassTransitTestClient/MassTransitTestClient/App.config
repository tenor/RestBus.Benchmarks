<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="ServerUri" value="rabbitmq://localhost/"/>
    <add key="Username" value="guest"/>
    <add key="Password" value="guest"/>
    <add key="MessageSize" value="2048"/>
    <add key="ExpectReply" value="false"/>
    <add key="NoOfThreads" value="20"/>
